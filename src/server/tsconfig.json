{
  "env":{
    "node":true,
    "es6":true
  },
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6"],
    "types": ["reflect-metadata","node"],
    "typeRoots": [
      "node_modules/@types"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}